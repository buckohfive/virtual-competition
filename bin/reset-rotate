#!/bin/bash

LOGROOT="/vagrant/logs"
if [ ! -d $LOGROOT ]; then
    mkdir $LOGROOT
fi

if ! which realpath >/dev/null; then
    # PLEASE
    REALROOT=/vagrant
else
    REALROOT=$(dirname $(dirname $(realpath $0)))
fi

echo $(date): $0 $@ >> $LOGROOT/launch.log

mv $REALROOT/shared/cgc-challenges/* $REALROOT/shared/cgc-challenges-unfielded 2>/dev/null
mv $REALROOT/shared/cgc-challenges-spent/* $REALROOT/shared/cgc-challenges-unfielded 2>/dev/null
